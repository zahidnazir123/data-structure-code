#include<stdio.h>
Struct node
{
Int data;
Struct node*next;
};
Struct node *head=null,*tail=null,*temp;
Int main()
{
Int ch; while(1)
{
Printf("1 for create\n 2 for view\n 3 for delete\n 4 for insert\n 5 for exit\n ur choice");
Scanf ("%d", &ch);
If(ch==1) create();
else if (ch==2) view5();
else if(ch==3) delete();
else if (ch==4) insert();
else break;
}
}
